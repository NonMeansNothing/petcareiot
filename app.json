{
  "expo": {
    "name": "petcareiot",
    "slug": "petcareiot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/catty.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#808080",
    "splash": {
      "image": "./assets/images/file.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#808080"
      },
      "package": "com.anonymous.petcareiot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-wifi-reborn"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
